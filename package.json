{
  "name": "kempe",
  "version": "0.0.0",
  "description": "Planar graph coloring using Kempe's algorithm",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "preversion": "npm test",
    "prepublish": "npm run tsc",
    "prepare": "husky",
    "tsc": "rm -rf dist && tsc",
    "test": "eslint . && prettier --check . && vitest run"
  },
  "author": "Jeffery Mensah",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.3.0",
    "husky": "^9.0.11",
    "lerna": "^8.1.3",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0",
    "vitest": "^1.6.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "graphology": "^0.25.4"
  }
}
